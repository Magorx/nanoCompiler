	func qsort[var arr = arr][var size = size] {
		var i = 0;
		var j = 0;
		var flag = 1;

		/*
			i = 0;
			>| (i < size) { printn[arr[i]]; i = i + 1; }
		*/

		i = 0;
		>| (i < size && flag) {
			j = 0;
			flag = 0;
			>| (j < size) {
				? (arr[i] < arr[j]) {
					var tmp = arr[i];
					arr[i] = arr[j];
					arr[j] = tmp;
					flag = 1;
				}
				j = j + 1;
			}
			i = i + 1;
		}
	}

